<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Student Homepage</title>

    <!-- font awesome cdn link  -->
   <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.1/css/all.min.css">

   <!-- custom css file link  -->
   <link rel="stylesheet" href="/public/styles.css">
   
</head>
<body>
    <%- include ('header') %>
    <section class="subjects">
        <h1 class="heading">Subjects</h1>
        <div class="box-container">
            <% rowdata.forEach((subjects) => { %>
                <div class="box" data-subjects-id="<%= subjects.id %>">
                    <h1 class="sub_name"><%= subjects.name %></h1>
                    <img src="<%= subjects.image %>" class="thumbnail">
                    <a href="topics" class="inline-btn">View Topics</a>
                </div>
            <% }) %>
        </div>
    </section>
    <%- include ('footer') %>
</body>
</html>